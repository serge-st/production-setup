{
  "name": "production-setup",
  "version": "1.0.0",
  "description": "",
  "main": "index.ts",
  "scripts": {
    "start": "webpack serve --env PORT=3000 --env DEV_SERVER=true",
    "build:dev": "webpack --env MODE=development",
    "build:prod": "webpack --env MODE=production",
    "build:prod:ci": "webpack --env MODE=production --env CI_BUILD=true",
    "lint:ts": "npx eslint . --ext .ts --ext .tsx",
    "lint:ts-fix": "npx eslint . --ext .ts --ext .tsx --fix",
    "lint:css": "npx stylelint \"**/*.scss\"",
    "test:unit": "jest --config=./config/jest/jest.config.ts",
    "test:screenshot": "npx loki test",
    "test:screenshot:ci": "npm run storybook:build:ci && loki --requireReference --reactUri file:./storybook-static",
    "test:screenshot:report": "npx reg-cli .loki/current .loki/reference .loki/difference -R .loki/report.html -J .loki/reg.json",
    "test:screenshot:addref": "npx loki",
    "test:screenshot:approve": "npx loki approve",
    "storybook": "storybook dev -p 6006 -c ./config/.storybook",
    "storybook:build": "storybook build -c ./config/.storybook",
    "storybook:build:ci": "storybook build -c ./config/.storybook --quiet",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/serge-st/production-setup.git"
  },
  "keywords": [],
  "author": "Serge Stecenko",
